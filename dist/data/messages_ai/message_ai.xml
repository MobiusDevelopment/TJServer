<?xml version='1.0' encoding='utf-8'?>
<list>
	<!-- когда моб первый раз фокусится на цели -->
	<package name="DefaultNoticeMessages" >
		<msg text="Не подходи!" />
		<msg text="Отвали я АФК!" />
		<msg text="Не трогай меня!" />
		<msg text="Зачем ты такое большое и страшное?" />
		<msg text="Не не не не не! Отвали я тя не потяну!" />
		<msg text="Что ты хочешь от меня!?" />
		<msg text="Ты мне не нравишься..." />
		<msg text="Ну с таким оружием и я кого хочешь порву..." />
		<msg text="Занято!" />
		<msg text="*обосрался*" />

		<msg text="Зачем ты такое маленькое и слабое?" />
		<msg text="Кулак видишь? Нет? Ну на поближе посмотри!" />
		<msg text="Да я тебя щас на хвост намотаю!" />
		<msg text="Ну-ка ну-ка, кто это тут у нас?" />
		<msg text="Леща поймаешь?" />
		<msg text="Что-то я тебя раньше тут не видел..." />
		<msg text="Семки есть? А 5 рублей? Есть? А позвонить?" />
		<msg text="Здесь нубам нельзя! Пшел вон отсюда!" />
		<msg text="Интересно, какой у тебя дроп будет?" />
	</package>
	
	<!-- когда моб разворачивается за целью для фокуса -->
	<package name="DefaultUpdateNoticeMessages" >
		<msg text="Ты пытаешься меня обмануть!?" />
		<msg text="Я тебе не дам меня испугать!" />
		<msg text="Ты думаешь я дурак!?" />
		<msg text="Ты куда собрался!?" />
		<msg text="Ты думаешь, я с тобой в шутки играю!?" />
		<msg text="Твоя попытка убить меня, ничтожна!" />
		<msg text="Зачем тебе это? Тебе меня не убить" />
		<msg text="Сейчас ты поплатишься за это!" />
		<msg text="Я давно тебя ждал!" />
		<msg text="И какой же ты на вкус?" />
		<msg text="Я соберу всех, и мы тебя уничтожим." />
		<msg text="Сегодня ты станишь моим завтраком." />
		<msg text="Вы ничтожественные создания..." />
		<msg text="За что... " />
		<msg text="Отведай силу моего гнева!" />
		<msg text="Вздумал в спину бить? Как не хорошо..." />
		<msg text="По твоему я слабый!?" />
		<msg text="Нет...Я не хочу умирать..." />
		<msg text="Что-то ты меня настораживаешь..." />
		<msg text="Мы еще с тобой увидимся, я не последний..." />
		<msg text="И это все на что ты способен!??" />
	</package>
	
	<!-- когда моб бродит -->
	<package name="DefaultWalkMessages" >
		<msg text="Как-то скучно здесь у нас..." />
		<msg text="Куда все подевались?" />
		<msg text="Какая хорошая погодка!" />
		<msg text="Мне кажется, или меня все игнорируют?" />
	</package>
	
	<package name="DefaultSwitchTargetMessages" >
		<msg text="Сейчас ты у меня получишь!" />
		<msg text="Иди комне!" />
		<msg text="Как ты посмел попасться мне на глаза!?" />
		<msg text="Пришел твой черед!" />
	</package>
	
	<package name="DefaultJumpSideMessages" >
		<msg text="Не попадешь!" />
	</package>
	
	<package name="DefaultJumpBehindMessages" >
		<msg text="Отходим!" />
	</package>
	
	<package name="DefaultShortAttackMessages" >
		<msg text="Сейчас ты получишь!" />
	</package>
	
	<package name="DefaultLongAttackMessages" >
		<msg text="Получай!" />
	</package>
	
	<package name="DefaultRunAwayMessages" >
		<msg text="Сваливаем, посоны, они с читами!" />
		<msg text="Да ну тебя нафиг!" />
		<msg text="Опа, я ж забыл дома плиту вырубить." />
		<msg text="SOS! SOS!" />
		<msg text="Кто-нибудь, позвоните в Гринпис!!" />
		<msg text="Милиция!" />
		<msg text="Мой дроп! Не отдам!!" />
		<msg text="Ну-ка догони!" />
		<msg text="Ой, что-то у меня живот разболелся." />
		<msg text="Что-то у меня голова разболелась." />
		<msg text="Да я щас апнусь и навешаю тебе!" />
		<msg text="Хилу мне! ХИЛУ!!" />
		<msg text="Ну я так не играю." />
		<msg text="В этот раз точно не догонят." />
		<msg text="Я щас, быстро, покурить куплю за углом и вернусь." />
		<msg text="Братва, выручайте!" />
		<msg text="Да ты ж меня щас убьешь нафиг!" />
		<msg text="А давай лучше поиграем в догонялки." />
		<msg text="Да ты больной!" />
		<msg text="Я твой дом труба шатал!!" />
		<msg text="Ветеринара мне! Срочно!" />
		<msg text="Тьфу ты! Мерзкий какой попался." />
		<msg text="Я устал. Я ухожу." />
		<msg text="Пробежимся?" />
		<msg text="Слуш, отвали, а?" />
		<msg text="Голды тебе? Накася выкуси!" />
		<msg text="Всё! Надоело! Завтра увольняюсь!" />
		<msg text="Я на это не подписывался." />
		<msg text="Ох ё!" />
		<msg text="Не подскажете, где здесь можно найти стаматолога?" />
		<msg text="Как же эти гопники задолбали уже." />
		<msg text="Да нафиг ты мне сдался?" />
		<msg text="Да ты чо? Больно же!" />
		<msg text="Да лучше я пойду травки пожую." />
		<msg text="*обосрался*" />
		<msg text="Они сказали: познакомишься с новыми людьми, увидишь свет. Ага. ТОТ свет!" />
		<msg text="Экспы хочешь? А у меня дома семья, дети! Ты о них подумал?" />
		<msg text="Нас не догонят! Нас не догонят!" />
		<msg text="Не хочу умирать девственником!" />
		<msg text="Неееет!" />
		<msg text="МАМА!" />
		<msg text="А говорили, что тут игроки лагают. Опять наврали." />
		<msg text="Не не не не!" />
	</package>
</list>